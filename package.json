    "watch": "tsc --watch",
    "prepack": "npm run clean && npm run build",
    "start": "tsc && node dist/index.js",
    "ts2c": "ts2c",
    "test": "jest",
    "lint": "tslint --project . --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Freuhlon/gbts"
  },
  "keywords": [
    "CLI",
    "GameBoy",
    "TypeScript"
  ],
  "dependencies": {
    "adm-zip": "0.5.1",
    "minimist": "1.2.5",
    "ora": "5.1.0",
    "request": "2.88.0",
    "ts2c": "2.2.9",
    "yargs": "16.1.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@types/minimist": "1.2.1",
    "@types/node": "14.14.11",
    "eslint": "6.8.0",
    "install": "0.13.0",
    "jest": "^26.6.3",
    "npm": "6.14.9",
    "rimraf": "^3.0.2",
    "shx": "0.3.3",
    "ts-jest": "^26.4.4",
    "tslint": "^5.12.1",
    "typescript": "3.9.7"
