{
  "name": "gbts",
  "version": "1.1.0",
  "description": "CLI tool that allows you to write GameBoy games on TypeScript !",
  "author": "Ang√©lo LIMA",
  "license": "ISC",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "./dist/src/index.d.ts",
  "bin": {
    "gbts": "./dist/index.js"
  },
  "scripts": {
    "install": "node install.js",
    "clean": "shx rm -rf dist/*",
